<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cursoandroid.cursoandroid.tenda_virtual">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Intents.FinalizarPedido"></activity>
        <activity android:name=".Intents.ZonaAdministrador" />
        <activity android:name=".Intents.VerComprasRealizadas" />
        <activity android:name=".Intents.VerPedidosEnTramite" />
        <activity android:name=".Intents.FacerPedido" />
        <activity android:name=".Intents.ZonaClientes" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>